{{#> layout}}
    <g transform="translate({{spec.profile.padding.x}}, {{spec.profile.padding.y}})">
        <g transform="translate(80,80)">
            {{#with god_concept}}
                {{gauge circle.R circle.r (object tl=6 tr=6 bl=6 br=6) (object start=circle.angles.start end=circle.angles.end) direction fill="#F3F4F6"}}
                {{gauge circle.R circle.r (object tl=6 tr=0 bl=6 br=0) (object start=circle.angles.start end=zeta) direction  fill=color}}
                <text y="-10" text-anchor="middle" dy=".3em" font-size="32" font-weight="500" fill="{{color}}">٪{{percentage}}</text>
                <text y="20" text-anchor="middle" dy=".3em" font-size="18" font-weight="400" fill="{{color}}">{{mark}}</text>
                <text y="-68" x="-12" text-anchor="middle" dy=".3em" font-size="14" font-weight="500" fill="#6B7280">0</text>
                <text y="-14" x="-68" text-anchor="middle" dy=".3em" font-size="14" font-weight="500" fill="#6B7280">{{max}}</text>
                <text y="98" text-anchor="middle" dy=".90em" font-size="18" font-weight="500" fill="{{color}}">{{label}}</text>
            {{/with}}
        </g>
        <g transform="translate(340,80)">
            {{#with god_image}}
                {{gauge circle.R circle.r (object tl=6 tr=6 bl=6 br=6) (object start=circle.angles.start end=circle.angles.end) direction fill="#F3F4F6"}}
                {{gauge circle.R circle.r (object tl=6 tr=0 bl=6 br=0) (object start=circle.angles.start end=zeta) direction  fill=color}}
                <text y="-10" text-anchor="middle" dy=".3em" font-size="32" font-weight="500" fill="{{color}}">٪{{percentage}}</text>
                <text y="20" text-anchor="middle" dy=".3em" font-size="18" font-weight="400" fill="{{color}}">{{mark}}</text>
                <text y="-68" x="-12" text-anchor="middle" dy=".3em" font-size="14" font-weight="500" fill="#6B7280">0</text>
                <text y="-14" x="-68" text-anchor="middle" dy=".3em" font-size="14" font-weight="500" fill="#6B7280">{{max}}</text>
                <text y="98" text-anchor="middle" dy=".90em" font-size="18" font-weight="500" fill="{{color}}">{{label}}</text>
            {{/with}}
        </g>
        <line x1="0" x2="400" y1="250" y2="250" stroke="#F3F4F6" />
        <g transform="translate(80,379)">
            {{#with self_concept}}
                {{gauge circle.R circle.r (object tl=6 tr=6 bl=6 br=6) (object start=circle.angles.start end=circle.angles.end) direction fill="#F3F4F6"}}
                {{gauge circle.R circle.r (object tl=6 tr=0 bl=6 br=0) (object start=circle.angles.start end=zeta) direction fill=color}}
                <text y="-10" text-anchor="middle" dy=".3em" font-size="32" font-weight="500" fill="{{color}}">٪{{percentage}}</text>
                <text y="20" text-anchor="middle" dy=".3em" font-size="18" font-weight="400" fill="{{color}}">{{mark}}</text>
                <text y="-68" x="-12" text-anchor="middle" dy=".3em" font-size="14" font-weight="500" fill="#6B7280">0</text>
                <text y="-14" x="-68" text-anchor="middle" dy=".3em" font-size="14" font-weight="500" fill="#6B7280">{{max}}</text>
                <text y="98" text-anchor="middle" dy=".90em" font-size="18" font-weight="500" fill="{{color}}">{{label}}</text>
            {{/with}}
        </g>
        <g transform="translate(340,379)">
            {{#with self_image}}
                {{gauge circle.R circle.r (object tl=6 tr=6 bl=6 br=6) (object start=circle.angles.start end=circle.angles.end) direction fill="#F3F4F6"}}
                {{gauge circle.R circle.r (object tl=6 tr=0 bl=6 br=0) (object start=circle.angles.start end=zeta) direction fill=color}}
                <text y="-10" text-anchor="middle" dy=".3em" font-size="32" font-weight="500" fill="{{color}}">٪{{percentage}}</text>
                <text y="20" text-anchor="middle" dy=".3em" font-size="18" font-weight="400" fill="{{color}}">{{mark}}</text>
                <text y="-68" x="-12" text-anchor="middle" dy=".3em" font-size="14" font-weight="500" fill="#6B7280">0</text>
                <text y="-14" x="-68" text-anchor="middle" dy=".3em" font-size="14" font-weight="500" fill="#6B7280">{{max}}</text>
                <text y="98" text-anchor="middle" dy=".90em" font-size="18" font-weight="500" fill="{{color}}">{{label}}</text>
            {{/with}}
        </g>
    </g>

{{/layout}}